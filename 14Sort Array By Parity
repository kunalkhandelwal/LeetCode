        l=0 # it is not nums[]
        
        #[3,1,5,2,7,4]
        #[3,1,5,2,7,9,4]
        
        r=len(nums)-1
        while(nums[r]%2!=0 and r>=0):
            r=r-1
        
        while l<r:
            if nums[l]%2 != 0:
                (nums[l], nums[r]) = (nums[r], nums[l])
                r=r-1
                while(nums[r]%2!=0 and r>=0):
                    r=r-1
                #print(nums)
            l=l+1
        return nums
